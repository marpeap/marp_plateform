    <script src="https://unpkg.com/aos@2.3.1/dist/aos.css"></script>
    <script>
        AOS.init({ duration: 800, once: true });

        // Script Sp√©cifique pour g√©rer l'envoi EmailJS et la date
        document.addEventListener('DOMContentLoaded', function() {
            // 1. Gestion de l'affichage du champ budget
            function toggleBudgetField() {
                const serviceInput = document.getElementById('service');
                const budgetContainer = document.querySelector('.budget-container');

                if (serviceInput && budgetContainer) {
                    const selectedValue = serviceInput.value;
                    if (selectedValue === 'Projet Sur-Mesure') {
                        budgetContainer.style.display = 'block';
                    } else {
                        budgetContainer.style.display = 'none';
                        // Reset budget to default when hidden
                        const budgetInput = document.getElementById('budget');
                        const budgetValue = document.getElementById('budget-value');
                        if (budgetInput) {
                            budgetInput.value = 'Non d√©fini';
                        }
                        if (budgetValue) {
                            budgetValue.textContent = 'Je ne sais pas';
                        }
                        // Reset selected state in modal
                        document.querySelectorAll('#budget-modal .form-option').forEach(opt => {
                            opt.classList.remove('selected');
                        });
                        document.querySelector('#budget-modal .form-option[data-value="Non d√©fini"]')?.classList.add('selected');
                    }
                }
            }

            // 2. Gestion des modales du formulaire
            function initFormModals() {
                // Gestionnaire pour ouvrir les modales
                document.querySelectorAll('.form-select-btn').forEach(button => {
                    button.addEventListener('click', function() {
                        const modalId = this.getAttribute('data-modal');
                        const modal = document.getElementById(modalId);
                        if (modal) {
                            modal.classList.add('active');
                            document.body.style.overflow = 'hidden';
                        }
                    });
                });

                // Gestionnaire pour fermer les modales
                document.querySelectorAll('.form-modal-close').forEach(button => {
                    button.addEventListener('click', function() {
                        const modal = this.closest('.form-modal');
                        modal.classList.remove('active');
                        document.body.style.overflow = 'auto';
                    });
                });

                // Fermeture en cliquant sur l'arri√®re-plan
                document.querySelectorAll('.form-modal').forEach(modal => {
                    modal.addEventListener('click', function(e) {
                        if (e.target === this) {
                            this.classList.remove('active');
                            document.body.style.overflow = 'auto';
                        }
                    });
                });

                // Gestionnaire pour s√©lectionner une option
                document.querySelectorAll('.form-option').forEach(option => {
                    option.addEventListener('click', function() {
                        const modal = this.closest('.form-modal');
                        const modalId = modal.id;
                        const value = this.getAttribute('data-value');
                        const text = this.textContent;

                        // Retirer la classe selected de tous les autres boutons
                        modal.querySelectorAll('.form-option').forEach(opt => {
                            opt.classList.remove('selected');
                        });

                        // Ajouter la classe selected au bouton cliqu√©
                        this.classList.add('selected');

                        // Mettre √† jour le champ cach√© et le bouton
                        const fieldName = modalId.replace('-modal', '');
                        const hiddenInput = document.getElementById(fieldName);
                        const displayValue = document.getElementById(fieldName + '-value');

                        if (hiddenInput) {
                            hiddenInput.value = value;
                        }
                        if (displayValue) {
                            displayValue.textContent = text;
                        }

                        // Fermer la modale
                        modal.classList.remove('active');
                        document.body.style.overflow = 'auto';

                        // Gestion sp√©ciale pour le champ budget
                        if (fieldName === 'service') {
                            setTimeout(toggleBudgetField, 10); // D√©lai pour s'assurer que la valeur est mise √† jour
                        }
                    });
                });

                // Fermeture avec la touche √âchap
                document.addEventListener('keydown', function(e) {
                    if (e.key === 'Escape') {
                        document.querySelectorAll('.form-modal.active').forEach(modal => {
                            modal.classList.remove('active');
                            document.body.style.overflow = 'auto';
                        });
                    }
                });
            }

            // Initialiser les modales et v√©rifier l'√©tat initial
            initFormModals();
            toggleBudgetField();

            // 2. Remplir la date cach√©e automatiquement
            const dateInput = document.getElementById('currentDate');
            if(dateInput) {
                const now = new Date();
                dateInput.value = now.toLocaleDateString('fr-FR') + ' ' + now.toLocaleTimeString('fr-FR');
            }

            // 2. Gestionnaire d'envoi du formulaire
            const form = document.getElementById('contactForm');
            const btn = document.getElementById('submitBtn');
            const msgBox = document.getElementById('formMessage');

            if(form) {
                form.addEventListener('submit', function(event) {
                    event.preventDefault();
                    
                    // √âtat "Envoi en cours..."
                    const originalBtnText = btn.innerHTML;
                    btn.innerHTML = 'Envoi en cours...';
                    btn.disabled = true;

                    // Envoi via EmailJS
                    // NOTE: Remplace 'SERVICE_ID' et 'TEMPLATE_ID' par tes vrais IDs EmailJS
                    emailjs.sendForm('ton_service_id', 'ton_template_id', this)
                        .then(function() {
                            // Succ√®s
                            msgBox.innerHTML = 'Message envoy√© avec succ√®s ! Je vous recontacte tr√®s vite.';
                            msgBox.className = 'form-message success show';
                            form.reset();
                            btn.innerHTML = originalBtnText;
                            btn.disabled = false;
                        }, function(error) {
                            // Erreur
                            console.error('Erreur EmailJS:', error);
                            msgBox.innerHTML = 'Une erreur est survenue. Appelez le Studio au 02 30 96 40 30.';
                            msgBox.className = 'form-message error show';
                            btn.innerHTML = originalBtnText;
                            btn.disabled = false;
                        });
                });
            }
        });
    </script>
